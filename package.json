    "cypress": "^5.4.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.6.0",
    "prettier": "2.1.2",
    "react-test-renderer": "^17.0.0",
    "start-server-and-test": "^1.11.5"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby-starter-hello-world"
